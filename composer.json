{
    "name": "nodes/core",
    "description": "Required for all Nodes packages",
    "keywords": [
        "nodes",
        "core",
        "helpers",
        "tools",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "http://nodesagency.com",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Morten Rugaard",
            "email": "moru@nodes.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0",
        "laravel/framework": "5.1.*||5.2.*",
        "garetjax/phpbrowscap": "^2.0"
    },
    "autoload": {
        "psr-4": {
           "Nodes\\": "src"
        },
        "files": [
            "src/Support/Helpers/Config.php",
            "src/Support/Helpers/Filesystem.php",
            "src/Support/Helpers/Request.php",
            "src/Support/Helpers/String.php",
            "src/Support/Helpers/UserAgent.php"
        ]
    },
    "extra": {
        "class": "Nodes\\Support\\Composer"
    }
}